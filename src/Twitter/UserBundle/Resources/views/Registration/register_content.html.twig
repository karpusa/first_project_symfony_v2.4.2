{% trans_default_domain 'FOSUserBundle' %}
<form action="{{ path('fos_user_registration_register') }}" {{ form_enctype(form) }} method="POST" class="form-signin">
    <h2 class="form-signin-heading">Впервые?</h2>
    {{ form_widget(form) }}
  <button type="submit" class="btn btn-lg btn-primary btn-block" value="{{ 'registration.submit'|trans }}">{{ 'registration.submit'|trans }}</button>        
</form>
